{
  "name": "dinnermatch-api-integration",
  "version": "1.0.0",
  "description": "API integration testing for DinnerMatch restaurant and recipe APIs",
  "type": "module",
  "scripts": {
    "test": "node --experimental-json-modules test-runner.js",

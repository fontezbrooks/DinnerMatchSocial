{
  "expo": {
    "name": "DinnerMatch Performance Test",
    "slug": "dinnermatch-perf-test",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-reanimated/plugin",
        {
          "relativeSourceLocation": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
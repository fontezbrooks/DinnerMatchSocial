# Authentication - Get from Clerk Dashboard
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key_here

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3001/api
EXPO_PUBLIC_API_TIMEOUT=10000

# Google OAuth - Get from Google Cloud Console
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here
EXPO_PUBLIC_GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Facebook OAuth - Get from Facebook Developer Console
EXPO_PUBLIC_FACEBOOK_APP_ID=your_facebook_app_id_here

# Google Maps API - Get from Google Cloud Console
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Deep Linking - Your app's URL scheme
EXPO_PUBLIC_SCHEME=dinnermatch

# Environment
EXPO_PUBLIC_ENVIRONMENT=development

# Feature Flags
EXPO_PUBLIC_ENABLE_SOCIAL_LOGIN=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_ANALYTICS=false